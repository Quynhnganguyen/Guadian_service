<h1>Entry</h1>
<% @entry %>
<div style='float: left;'>
  <img class="img-rounded" <%= image_tag @entry.image.try(:url), width: 500, height: 500%>
</div>
<div style = 'float: left;'
    <form class="form-horizontal">
      <div class="control-group">
        <label class="control-label" for="inputEmail">Email's guard</label>
        <div class="controls">
          <input class="span4" id='inputEmail'type="text" value = "<%= @entry.user.try(:email)%>">
        </div>
      </div>
      <div class="control-group">
        <label class="control-label">Tag</label>
        <div class="controls">
          <input class="span4" type="text" value = "<%= @entry.tag%>">
        </div>
      </div>
      <div class="control-group">
        <label class="control-label">Check in at</label>
        <div class="controls">
          <input class="span4" type="text" value= "<%= @entry.check_in_at%>">
        </div>
      </div>
      <div class="control-group">
        <label class="control-label">Check out at</label>
        <div class="controls">
          <input class="span4" type="text" value= "<%= @entry.check_out_at%>" >
        </div>
      </div>
    </form>
</div>