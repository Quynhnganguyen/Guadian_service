<h1>Entries</h1>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>Tag</th>
      <th>Image</th>
      <th>User</th>
      <th>Time Check In</th>
      <th>Time Check Out</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @entries.each do |e| %>
      <tr>
        <td><%= e.tag %></td>
        <td><%= image_tag e.image.try(:url), width: 100, height: 100 %></td>
        <td><%= e.user.try(:email) %></td>
        <td><%= e.check_in_at %></td>
        <td><%= e.check_out_at %></td>
        <td></td>
      </tr>
    <% end %>
  </tbody>
</table>